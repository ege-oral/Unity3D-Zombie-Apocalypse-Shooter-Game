
class Person:
    name = ""
    age = 0
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    def info(self):
        print(f"My name is {self.name} and my age is {self.age}.")


class Me(Person):
    
    def __init__(self, name, age):
        super().__init__(name, age)

m = Me("ege", 21)
m.info()